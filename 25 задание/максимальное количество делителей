max_divs = 0
for i in range(586132, 586431):
    divs = set()
    for d in range(1, round(i ** 0.5) + 1):
        if i % d == 0:
            divs.add(d)
            divs.add(i // d)
    max_divs = max(max_divs, len(divs))
print(max_divs)
for i in range(586132, 586431):
    divs = set()
    for d in range(1, round(i ** 0.5) + 1):
        if i % d == 0:
            divs.add(d)
            divs.add(i // d)
    if len(divs) == max_divs:
        print(i, sorted(divs))
